        {
            "id": "object_detection",
            "name": "Object detection",
            "description": "Label detection with boundingboxes",
            "model_id": "amazon.nova-lite-v1:0",
            "prompt": "Detect objects in the image and return their coordinates. If the frame contains a person, determine if the person is facing the camera or not. Consider a person as \"facing the camera\" if their face or front torso is visible and oriented toward the viewer",
            "toolConfig": {
                "toolChoice": {
                    "tool": {
                        "name": "tool_result_with_bb"
                    }
                },
                "tools": [
                    {
                        "toolSpec": {
                            "name": "tool_result_with_bb",
                            "description": "Detect objects in the image and return their coordinates. For person objects, also determine if they are facing the camera.",
                            "inputSchema": {
                                "json": {
                                    "type": "object",
                                    "properties": {
                                        "objects": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string",
                                                        "enum": [
                                                            "person",
                                                            "car",
                                                            "package"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "object",
                                                        "properties": {
                                                            "x": {
                                                                "type": "number",
                                                                "description": "X coordinate of top-left corner"
                                                            },
                                                            "width": {
                                                                "type": "number",
                                                                "description": "Width of bounding box"
                                                            },
                                                            "y": {
                                                                "type": "number",
                                                                "description": "Y coordinate of top-left corner"
                                                            },
                                                            "height": {
                                                                "type": "number",
                                                                "description": "Height of bounding box"
                                                            }
                                                        },
                                                        "required": [
                                                            "x",
                                                            "y",
                                                            "width",
                                                            "height"
                                                        ]
                                                    },
                                                    "facingCamera": {
                                                        "type": "boolean",
                                                        "description": "Only applicable for 'person' objects. True if person is facing the camera, false otherwise."
                                                    }
                                                },
                                                "required": [
                                                    "name",
                                                    "coordinates"
                                                ]
                                            }
                                        }
                                    },
                                    "required": [
                                        "objects"
                                    ]
                                }
                            }
                        }
                    }
                ]
            },
